{
  "name": "multicontentsmanager",
  "version": "0.0.1",
  "description": "A JupyterLab extension.",
  "author": "Tim Paine",
  "main": "lib/index.js",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "scripts": {
    "build": "tsc",
    "build:lab": "rimraf lab-dist && mkdirp lab-dist && cd lab-dist && npm pack ..",
    "build:all": "npm run build && npm run build:lab",
    "clean": "rimraf lib",
    "fix": "./node_modules/.bin/tslint --fix src/* src/*/*",
    "lint": "./node_modules/.bin/tslint './src/*.ts'",
    "prepublishOnly": "npm run build",
    "test": "jest --coverage --collectCoverageFrom=src/*.{ts}"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,svg}"
  ],
  "jupyterlab": {
    "extension": true,
    "discovery": {
      "server": {
        "base": {
          "name": "multicontentsmanager"
        },
        "managers": [
          "pip"
        ]
      }
    }
  },
  "dependencies": {
    "@jupyterlab/application": "^2.0.0",
    "@jupyterlab/docmanager": "^2.0.0",
    "@jupyterlab/filebrowser": "^2.0.0",
    "@jupyterlab/services": "^5.0.0",
    "@lumino/algorithm": "^1.2.3",
    "@types/file-saver": "^2.0.1",
    "@types/jszip": "^3.1.6",
    "file-saver": "^2.0.2",
    "jszip": "^3.2.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.5.5",
    "@types/jest": "^24.0.4",
    "babel-jest": "^24.8.0",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^24.1.0",
    "jest-transform-css": "^2.0.0",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.6.3",
    "ts-jest": "^23.10.4",
    "tslint": "^5.20.0",
    "typescript": "^3.5.3"
  }
}
